#include <iostream> 
using namespace std; 

// Linearly search x in arr[]. If x is present then return its 
// location, otherwise return -1 
int search(int arr[], int n, int x) 
{ 
	int i; 
	for (i = 0; i < n; i++) 
		if (arr[i] == x) 
			return i; 
	return -1; 
} 

// Driver code 
int main() 
{ 
	int arr[] = { 3, 2,1,8,17 }; 
	int n = sizeof(arr) / sizeof(arr[0]); 
	int x ;
	cin>>x;

	int index = search(arr, n, x); 
	if (index == -1) 
		cout << " NO,element found that you Search"; 
	else
		cout << "YES, There is a element you search at location" << index; 

	return 0; 
} 
